    <script>
(() => {
    let ele = document.querySelector('.survey-winners__header')
    if (ele) {
        ele.innerHTML = 'Recent Winners'
    }

    ele = document.querySelector('.bonus-game__header')
    if (ele) {
        ele.innerHTML = ele.innerHTML.replace('Big Gift Reveal', 'Ready Set Pop')
    }

})()
</script>

<script>
    (function(){
        const ele = document.querySelector('.uninav .uninav__top-bar__links')
        if(ele){
            for (let i = 0; i < ele.children.length; i++) {
                if(ele){
                    const txt = ele.children[i].querySelector('a').innerHTML.trim()
                    if(txt === 'Earn Tokens'){
                        ele.children[i].style.display = 'none'
                        break;
                    }
                }
            }
        }
    })()
</script>

<script>
function isIpadOS() {
    return navigator.maxTouchPoints &&
        navigator.maxTouchPoints > 2 &&
        /MacIntel/.test(navigator.platform);
}
if(isIpadOS() || PCH.ssoui === 'tablet'){
    var dropdownName = document.querySelectorAll('.sweepstakes__dropdown__list__item a .dropdown-name')
    for(var i=0; i<dropdownName.length; i++){
        dropdownName[i].style.fontSize = '10px'
    }
}
</script>

<script>
if(PCHUSER.type === 0){
    var progressDots = document.querySelectorAll('.progress-icon')
    for (var i = 0; i < progressDots.length; i++) {
        progressDots[i].classList.remove('checked')
     }
}
</script>


